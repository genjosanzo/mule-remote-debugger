<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.mule.tooling.core.contribution">
      <externalContribution
            contributionLibPathInMule="/plugins"
            contributionLibs="mule-debugger-3.2.0.zip"
            contributionNamespace="http://www.mulesoft.org/schema/mule/muledebugger"
            contributionNamespaceFile="http://www.mulesoft.org/schema/mule/muledebugger/3.2.0/mule-muledebugger.xsd"
            contributionNamespaceId="muledebugger"
            contributionNamespacePrefix="muledebugger"
            contributionType="component"
            path="debugger.xml"
            version="3.2.0">
      </externalContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="org.mule.tooling.ui.views"
            class="org.mule.tooling.ui.contribution.debugger.ui.MuleDebuggerView"
            icon="icons/small/debug.png"
            id="org.mule.tooling.ui.contribution.debugger.view"
            name="Mule Debugger View"
            restorable="true">
      </view>
   </extension>

</plugin>
